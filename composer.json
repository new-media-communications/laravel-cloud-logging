{
    "name": "nmc/laravel-cloud-logging",
    "description": "Google Cloud Logging For Laravel",
    "keywords": [
        "nmc",
        "laravel",
        "laravel-cloud-logging"
    ],
    "homepage": "https://github.com/new-media-communications/laravel-cloud-logging",
    "license": "MIT",
    "authors": [
        {
            "name": "Fitim Vata",
            "email": "fitimvata28@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "google/cloud-error-reporting": "^0.22.1",
        "google/cloud-logging": "^1.25.2",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.14.1"
    },
    "autoload": {
        "psr-4": {
            "Nmc\\CloudLogging\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nmc\\CloudLogging\\CloudLoggingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
